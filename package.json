{
  "name": "hanmadiro",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android:dev": "npm run bundle:remove && react-native run-android --mode=developmentdebug",
    "android:dev-release": "npm run bundle:remove && react-native run-android --mode=developmentrelease",
    "android:staging": "npm run bundle:remove && react-native run-android --mode=stagingdebug",
    "android:staging-release": "npm run bundle:remove && react-native run-android --mode=stagingrelease",
    "android:prod": "npm run bundle:remove && react-native run-android --mode=productiondebug",
    "android:prod-release": "npm run bundle:remove && react-native run-android --mode=productionrelease",
    "ios:dev": "npm run pod:install && react-native run-ios --scheme hanmadiroDev",
    "ios:dev:device": "npm run pod:install && react-native run-ios --scheme hanmadiroDev --device",
    "ios:staging": "npm run pod:install && react-native run-ios --scheme hanmadiroStaging",
    "ios:staging:device": "npm run pod:install && react-native run-ios --scheme hanmadiroStaging --device",
    "ios:prod": "npm run pod:install && react-native run-ios --scheme hanmadiroProduct",
    "ios:prod:device": "npm run pod:install && react-native run-ios --scheme hanmadiroProduct --device",
    "build:ios": "npm run pod:install",
    "bundle:ios": "react-native bundle --platform ios --entry-file index.js --bundle-output ios/hanmadiro/main.jsbundle --dev false --verbose",
    "bundle:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "bundle:remove": "rm -rf android/app/src/main/assets/index.android.bundle",
    "build:aab": "mkdir -p android/app/src/main/assets && npm run bundle:android && cd android && ./gradlew bundleProductionRelease && cd .. && open android/app/build/outputs/bundle/production/release",
    "build:apk": "mkdir -p android/app/src/main/assets && npm run bundle:android && cd android && ./gradlew assembleProductionRelease && cd .. && open android/app/build/outputs/apk/production/release",
    "gradle:clean": "cd android && ./gradlew clean && cd ../",
    "pod:install": "npm run pod:clean && react-native setup-ios-permissions && pod-install && cd ios && pod install && cd ../",
    "pod:clean": "cd ios && pod cache clean --all && cd ../",
    "pod:remove:all": "rm -rf ./ios/Pods/",
    "open:xcode": "open ios/hanmadiro.xcworkspace",
    "open:android": "open -a /Applications/Android\\ Studio.app ./android",
    "open:avd": "emulator -avd Pixel_5_API_32 -wipe-data",
    "avd:list": "emulator -list-avds",
    "format": "prettier --check ./src",
    "setup:ios:permission": "react-native setup-ios-permissions && pod-install",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@react-native-community/netinfo": "^11.3.2",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@tanstack/react-query": "^5.51.21",
    "axios": "^1.7.3",
    "lottie-react-native": "^6.7.2",
    "react": "18.2.0",
    "react-lottie": "^1.2.4",
    "react-native": "0.74.5",
    "react-native-config": "^1.5.3",
    "react-native-gesture-handler": "^2.18.1",
    "react-native-modal": "^13.0.1",
    "react-native-paper": "^5.12.5",
    "react-native-reanimated": "^3.15.0",
    "react-native-safe-area-context": "^4.10.8",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-svg": "^15.5.0",
    "react-native-svg-transformer": "^1.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-transform-arrow-functions": "^7.24.7",
    "@babel/plugin-transform-class-properties": "^7.24.7",
    "@babel/plugin-transform-private-methods": "^7.24.7",
    "@babel/plugin-transform-private-property-in-object": "^7.24.7",
    "@babel/plugin-transform-shorthand-properties": "^7.24.7",
    "@babel/plugin-transform-template-literals": "^7.24.7",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-native/babel-preset": "0.74.87",
    "@react-native/eslint-config": "0.74.87",
    "@react-native/metro-config": "0.74.87",
    "@react-native/typescript-config": "0.74.87",
    "@tanstack/eslint-plugin-query": "^5.51.15",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@tsconfig/react-native": "^3.0.5",
    "@types/react": "^18.2.6",
    "@types/react-lottie": "^1.2.10",
    "@types/react-native-snap-carousel": "^3.8.11",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
